# 服务器配置
SERVER_PORT=8080
CONTEXT_PATH=/api
SPRING_PROFILES_ACTIVE=dev

# 数据库配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=supermarket_db
MYSQL_USERNAME=your_username
MYSQL_PASSWORD=your_password

# Druid连接池配置
DRUID_INITIAL_SIZE=5
DRUID_MIN_IDLE=5
DRUID_MAX_ACTIVE=20
DRUID_MAX_WAIT=60000
DRUID_STAT_ENABLED=false
DRUID_ADMIN_USERNAME=admin
DRUID_ADMIN_PASSWORD=strong_password

# AI配置
AI_API_KEY=your_api_key
AI_MODEL=qwen-plus
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=2000

# JWT配置
JWT_SECRET=your_strong_secret_key
JWT_EXPIRATION=86400000
JWT_REFRESH_EXPIRATION=604800000

# 阿里云配置
ALICLOUD_ACCESS_KEY=your_access_key
ALICLOUD_SECRET_KEY=your_secret_key
ALICLOUD_REGION=cn-hangzhou
ALICLOUD_PAI_ENDPOINT=https://pai-eas.cn-hangzhou.aliyuncs.com

# 通义千问配置
TONGYI_API_KEY=your_tongyi_api_key
TONGYI_MODEL=qwen-plus
TONGYI_TEMPERATURE=0.3
TONGYI_MAX_TOKENS=2000
